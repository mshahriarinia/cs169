-# this file is /views/movies/index.html.haml

hi

%h2

%table#movies
  %thead
    %tr
      %th Movie Title
      %th Rating
      %th Release Date
      %th More Info
  %tbody
    - @movies.each do |m|
      %td= m.title
      %td= m.rating
      %td= m.release_date
      %td= link_to "Read more about #{m.title}", movie_path(m)

      

